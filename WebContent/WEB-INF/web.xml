<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>piatomcat</display-name>
	<servlet>
		<servlet-name>MyBatisInitializer</servlet-name>
		<servlet-class>com.zgd.mybatis.config.MyBatisConfig</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>GetShopList</servlet-name>
		<servlet-class>com.zgd.controller.GetShopList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetShopList</servlet-name>
		<url-pattern>/GetShopList</url-pattern>
	</servlet-mapping>
	<!-- 台番リスト取得 -->
	<servlet>
		<servlet-name>GetTaiNoList</servlet-name>
		<servlet-class>com.zgd.controller.GetTaiNoList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetTaiNoList</servlet-name>
		<url-pattern>/GetTaiNoList</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetTaiInfoList</servlet-name>
		<servlet-class>com.zgd.controller.GetTaiInfoList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetTaiInfoList</servlet-name>
		<url-pattern>/GetTaiInfoList</url-pattern>
	</servlet-mapping>
<servlet>
		<servlet-name>GetTaiInfoList2</servlet-name>
		<servlet-class>com.zgd.controller.GetTaiInfoList2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetTaiInfoList2</servlet-name>
		<url-pattern>/GetTaiInfoList2</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>3.html</welcome-file>
	</welcome-file-list>
</web-app>